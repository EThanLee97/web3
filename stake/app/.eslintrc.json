{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "next",
    "next/core-web-vitals",
    "plugin:prettier/recommended"
  ],
  "plugins": ["react", "prettier", "import"],
  "rules": {
    "prettier/prettier": "error",
    "react/prop-types": "off"
  },
  "ignorePatterns": ["node_modules/", ".next/", "out/"]
}
